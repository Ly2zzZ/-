{
  "name": "bilibili-tv",
  "version": "0.0.10",
  "manifest_version": 2,
  "description": "B站TV插件",
  "permissions": [
    "privacy",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "./pop/pop.html"
  },
  "devtools_page": "devtools.html",
  "content_scripts": [
    {
      "js": [
        "content_scripts.js"
      ],
      "css": [
      ],
      "matches": [
        "https://www.bilibili.com/**"
      ],
      "run_at": "document_end"
    }
  ]
}